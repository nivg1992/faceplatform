{
  "name": "faceplatform",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint . --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "prepare": "husky",
    "lint-stage:fix": "eslint --quiet --fix --ext .js --ext .jsx --ext .ts --ext .tsx",
    "format-stage": "prettier --write --stage \"**/*.+(js|ts|jsx|tsx|json|yml|yaml|css|md)\"",
    "format": "prettier --write . \"**/*.+(js|ts|jsx|tsx|json|yml|yaml|css|md)\""
  },
  "dependencies": {
    "@ant-design/icons": "^5.5.2",
    "antd": "^5.22.3",
    "axios": "^1.7.8",
    "globals": "^15.13.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^7.0.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.16.0",
    "@types/eslint__js": "^8.42.3",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@typescript-eslint/eslint-plugin": "^8.17.0",
    "@typescript-eslint/parser": "^8.17.0",
    "@vitejs/plugin-react": "^4.3.4",
    "eslint": "^9.16.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-prettier": "5.2.1",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.16",
    "husky": "^9.1.7",
    "prettier": "^3.4.1",
    "typescript": "^5.7.2",
    "typescript-eslint": "^8.17.0",
    "vite": "^6.0.2"
  },
  "lint-staged": {
    "*.+(js|ts|jsx|tsx|json|yml|yaml|css|md)": [
      "pnpm format-stage",
      "pnpm lint-stage:fix"
    ]
  }
}
