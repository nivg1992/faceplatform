{
  "name": "faceplatform",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint . --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "prepare": "husky",
    "lint-stage:fix": "eslint --quiet --fix --ext .js --ext .jsx --ext .ts --ext .tsx",
    "format-stage": "prettier --write --stage \"**/*.+(js|ts|jsx|tsx|json|yml|yaml|css|md)\"",
    "format": "prettier --write . \"**/*.+(js|ts|jsx|tsx|json|yml|yaml|css|md)\""
  },
  "dependencies": {
    "@ant-design/icons": "^5.6.1",
    "antd": "^5.24.1",
    "axios": "^1.7.9",
    "globals": "^15.15.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-router-dom": "^7.1.5"
  },
  "devDependencies": {
    "@eslint/js": "^9.20.0",
    "@types/eslint__js": "^8.42.3",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "@typescript-eslint/eslint-plugin": "^8.24.1",
    "@typescript-eslint/parser": "^8.24.1",
    "@vitejs/plugin-react": "^4.3.4",
    "eslint": "^9.20.1",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-prettier": "5.2.3",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "husky": "^9.1.7",
    "prettier": "^3.5.1",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.24.1",
    "vite": "^6.1.0"
  },
  "lint-staged": {
    "*.+(js|ts|jsx|tsx|json|yml|yaml|css|md)": [
      "pnpm format-stage",
      "pnpm lint-stage:fix"
    ]
  }
}
